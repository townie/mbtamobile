<div class="list-group ">

  <% @stops.each do |stop| %>
    <div class = "row">
      <div class="col-md-12">
        <a href="#" class="list-group-item">
         <%= stop["stop_name"]+ "-Distance:  " + sprintf("%.2f", stop["distance"]) + " miles" %>
        </a>
      </div>
  </div>
  <% end %>

</div>


